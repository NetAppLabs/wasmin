  "license": "MIT",
  "homepage": "https://github.com/tryggvil/shell#readme",
  "bugs": {
    "url": "https://github.com/tryggvil/shell/issues"
  },
  "contributors": [
    {
      "name": "tryggvil"
    }
  ],
  "devDependencies": {
    "@netapplabs/tsconfig": "^0.0.3",
    "eslint-config-custom": "*"
  },
  "dependencies": {
    "@netapplabs/fs-js": "^0.0.8",
    "@netapplabs/github-fs-js": "^0.1.1",
    "@netapplabs/nfs-js": "^0.1.5",
    "@netapplabs/node-fs-js": "^0.1.2",
    "@netapplabs/s3-fs-js": "^0.1.1",
