{
  "name": "@wasm-env/shell",
  "version": "0.0.1",
  "description": "WASI Shell",
  "type": "module",
  "source": "src/index.ts",
  "exports": "./dist/index.js",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "src",
    "dist"
  ],
  "scripts": {
    "build": "tsc -p .",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "dev": "tsc -p . --watch",
    "lint": "eslint \"./{src,tests}/**/*.{ts,mjs}\" --fix",
    "format": "prettier --tab-width 4 --print-width 120 \"./{src,tests}/**/*.{ts,mjs}\" --write",
    "start": "echo start"
  },
  "repository": "ssh://git@github.com/tryggvil/shell.git",
  "author": "Tryggvi Larusson",
  "license": "MIT",
  "homepage": "https://github.com/tryggvil/shell#readme",
  "bugs": {
    "url": "https://github.com/tryggvil/shell/issues"
  },
  "contributors": [
    {
      "name": "tryggvil"
    }
  ],
  "devDependencies": {
    "@wasm-env/tsconfig": "*",
    "eslint-config-custom": "*"
  },
  "dependencies": {
    "@wasm-env/fs-js": "*",
    "@wasm-env/github-fs-js": "*",
    "@wasm-env/node-fs-js": "*",
    "@wasm-env/s3-fs-js": "*",
    "@wasm-env/nfs-js": "*",
    "@wasm-env/wasi-js": "*"
  }
}
