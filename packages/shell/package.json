{
  "name": "@netapplabs/shell",
  "version": "0.0.1",
  "description": "WASI Shell",
  "type": "module",
  "main": "dist/index.js",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "run-s build:*",
    "build:tsc": "tsc -p .",
    "build:nu": "cp nu.async.wasm dist/",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "dev": "tsc -p . --watch",
    "lint": "eslint \"./{src,tests}/**/*.{ts,mjs}\" --fix",
    "format": "prettier --tab-width 4 --print-width 120 \"./{src,tests}/**/*.{ts,mjs}\" --write",
    "start": "echo start"
  },
  "repository": "ssh://git@github.com/tryggvil/shell.git",
