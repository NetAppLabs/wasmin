{
  "name": "@wasmin/fs-js",
  "version": "0.0.4",
  "description": "File System Standard Implementation",
  "type": "module",
  "source": "src/index.ts",
  "exports": "./dist/index.js",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsc -p .",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "dev": "tsc -p . --watch",
    "lint": "eslint \"./{src,tests}/**/*.{ts,mjs}\" --fix",
    "format": "prettier --tab-width 4 --print-width 120 \"./{src,tests}/**/*.{ts,mjs}\" --write",
    "start": "echo start",
    "test": "vitest run --globals",
    "test:ui": "vitest run --globals --ui"
  },
  "repository": "ssh://git@github.com/NetAppLabs/wasmin.git",
  "keywords": [
    "filesystem",
    "file",
    "blob",
    "stream",
    "fs",
    "read",
    "write",
    "delete",
    "file system access",
    "spec"
  ],
  "author": "tryggvil",
  "license": "MIT",
  "homepage": "https://github.com/NetAppLabs/wasmin#readme",
  "bugs": {
    "url": "https://github.com/NetAppLabs/wasmin/issues"
  },
  "contributors": [
    {
      "name": "jimmywarting"
    },
    {
      "name": "Alexandru Ciuca"
    },
    {
      "name": "kobakazu0429"
    },
    {
      "name": "tryggvil"
    }
  ],
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@types/filesystem": "^0.0.32",
    "@types/js-yaml": "^4.0.5",
    "@types/url-parse": "^1.4.8",
    "@vitest/coverage-v8": "^0.32.2",
    "@vitest/ui": "^0.32.2",
    "@wasmin/tsconfig": "*",
    "eslint-config-custom": "*",
    "vitest": "^0.34.3"
  },
  "dependencies": {
    "@types/jest": "^29.5.2",
    "buffer": "^6.0.3",
    "js-yaml": "^4.1.0",
    "parse-url": "^8.1.0",
    "url": "^0.11.0",
    "url-parse": "^1.5.10"
  }
}
