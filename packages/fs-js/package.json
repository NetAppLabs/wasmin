{
  "name": "@netapplabs/fs-js",
  "version": "0.0.7",
  "description": "File System Standard Implementation",
  "type": "module",
  "source": "src/index.ts",
  "exports": "./dist/index.js",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsc -p .",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "dev": "tsc -p . --watch",
    "lint": "eslint \"./{src,tests}/**/*.{ts,mjs}\" --fix",
    "format": "prettier --tab-width 4 --print-width 120 \"./{src,tests}/**/*.{ts,mjs}\" --write",
    "start": "echo start",
    "test": "vitest run --globals",
    "test:ui": "vitest run --globals --ui"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NetAppLabs/wasmin.git"
  },
  "keywords": [
    "filesystem",
    "file",
    "blob",
    "stream",
    "fs",
    "read",
    "write",
    "delete",
    "file system access",
    "spec"
  ],
  "author": "tryggvil",
  "license": "MIT",
  "homepage": "https://github.com/NetAppLabs/wasmin#readme",
  "bugs": {
    "url": "https://github.com/NetAppLabs/wasmin/issues"
  },
  "contributors": [
    {
      "name": "jimmywarting"
    },
    {
      "name": "Alexandru Ciuca"
    },
    {
      "name": "kobakazu0429"
    },
    {
      "name": "tryggvil"
    }
  ],
  "devDependencies": {
    "@types/filesystem": "^0.0.32",
    "@types/js-yaml": "^4.0.5",
    "@vitest/coverage-v8": "^2.1.8",
    "@vitest/ui": "^2.1.8",
    "@netapplabs/tsconfig": "^0.0.2",
    "eslint-config-custom": "*",
    "vitest": "^2.1.8"
  },
  "dependencies": {
    "@types/jest": "^29.5.12",
    "buffer": "^6.0.3",
    "js-yaml": "^4.1.0"
  }
}
