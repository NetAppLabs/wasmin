{
  "name": "@netapplabs/bun-fs-js",
  "version": "0.1.0",
  "description": "",
  "type": "module",
  "module": "src/index.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NetAppLabs/wasmin.git"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/*.js",
    "dist/*.d.ts"
  ],
  "scripts": {
    "build": "bun build --target=bun ./src/index.ts --outfile=dist/index.js && dts-bundle-generator -o dist/index.d.ts ./src/index.ts",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "dev": "tsc -p . --watch",
    "lint": "eslint \"./{src,tests}/**/*.{ts,mjs}\" --fix",
    "format": "prettier --tab-width 4 --print-width 120 \"./{src,tests}/**/*.{ts,mjs}\" --write",
    "start": "echo start",
    "test": "bun test",
    "bun-test": "bun test"
  },
  "author": "tryggvil",
  "dependencies": {
    "@netapplabs/fs-js": "^0.0.8"
  },
  "devDependencies": {
    "@netapplabs/tsconfig": "^0.0.3",
    "@types/bun": "latest",
    "eslint-config-custom": "*",
    "dts-bundle-generator": "^9.5.1"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  }
}
