{
  "name": "@netapplabs/wasm-promisify",
  "version": "0.10.0",
  "description": "Promisify Wasm for JSPI",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NetAppLabs/wasmin.git"
  },
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./*.js": {
      "types": "./dist/*.d.ts",
      "default": "./dist/*.js"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "scripts": {
    "build": "tsc -p .",
    "clean": "rm -rf node_modules && rm -rf dist"
  },
  "files": [
    "dist",
    "src"
  ],
  "devDependencies": {
    "@types/node": "^22.10.2"
  },
  "dependencies": {
    "@netapplabs/tsconfig": "^0.0.2",
    "wasmati": "^0.2.0"
  }
}
