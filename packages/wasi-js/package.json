{
  "name": "@wasm-env/wasi-js",
  "version": "0.0.2",
  "private": false,
  "type": "module",
  "source": "src/index.ts",
  "exports": "./dist/index.js",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsc -p .",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "dev": "tsc -p . --watch",
    "lint": "eslint \"./{src,tests}/**/*.{ts,mjs}\" --fix",
    "format": "prettier --tab-width 4 --print-width 120 \"./{src,tests}/**/*.{ts,mjs}\" --write",
    "start": "echo start",
    "test": "vitest run --globals",
    "lint-prettier": "npm run lint:eslint && npm run lint:prettier",
    "test-no-esm": "jest -i --silent=false --verbose false",
    "test-jest": "export NODE_OPTIONS=\"--experimental-vm-modules --trace-warnings\" && jest -i --silent=false --verbose=false",
    "build:esbuild": "esbuild --bundle src/index.ts --platform=browser --format=esm --outfile=dist/index.js",
    "dev:esbuild": "esbuild --bundle src/index.ts --format=esm --outfile=dist/index.js --watch",
    "lint:eslint": "eslint \"./{src,tests}/**/*.{ts,mjs}\" --fix",
    "lint:prettier": "prettier --tab-width 4 --print-width 120 \"./{src,tests}/**/*.{ts,mjs}\" --write"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NetAppLabs/wasm-env.git"
  },
  "keywords": [],
  "author": "Ingvar Stepanyan <rreverser@google.com>",
  "contributors": [
    {
      "name": "Ingvar Stepanyan",
      "email": "rreverser@google.com"
    },
    {
      "name": "kobakazu0429",
      "email": "kobakazu10@gmail.com"
    },
    {
      "name": "tryggvil",
      "email": "tryggvi@netapp.com"
    }
  ],
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/NetAppLabs/wasm-env/issues"
  },
  "homepage": "https://github.com/NetAppLabs/wasm-env#readme",
  "dependencies": {
    "@types/wicg-file-system-access": "^2020.9.5",
    "@wasm-env/fs-js": "*",
    "@wasm-env/node-fs-js": "*",
    "comlink": "^4.4.1",
    "events": "^3.3.0"
  },
  "devDependencies": {
    "@types/jest": "^29.4.0",
    "@vitest/coverage-c8": "^0.23.1",
    "@wasm-env/tsconfig": "*",
    "eslint-config-custom": "^0.0.0",
    "jest": "^29.4.3",
    "jest-extended": "^3.2.3",
    "ts-jest": "^29.0.5",
    "vitest": "^0.28.5",
    "web-file-polyfill": "^1.0.4",
    "web-streams-polyfill": "^3.1.1"
  }
}
