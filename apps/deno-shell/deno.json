    "compile-linux-x86_64": "deno compile --target x86_64-unknown-linux-gnu --include ./dist/wasiWorkerThread.js --include ./dist/wasmComponentWorkerThread.js --include ./dist/wasmCoreWorkerThread.js --v8-flags=--experimental-wasm-stack-switching,--experimental-wasm-type-reflection --allow-all --allow-env --unstable-net -o ./wasmin-deno --node-modules-dir=false ./src/index.ts"
