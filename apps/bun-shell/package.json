    "esbuild": "^0.14.39",
    "typescript": "^4.7.3",
    "vite": "^2.5.1"
  },
  "dependencies": {
    "@wasm-env/fs-js": "*",
    "@wasm-env/wasi-js": "*",
    "@wasm-env/bun-console": "*",
    "bun-utilities": "^0.2.1",
    "get-random-values": "^1.2.2",
    "web-file-polyfill": "^1.0.4"
  }
}