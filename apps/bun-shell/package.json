{
  "name": "@wasmin/bun-shell",
  "version": "0.0.0",
  "scripts": {
    "start": "bun src/index.ts",
    "build": "node esbuild.mjs",
    "build-tsc": "tsc -p .",
    "build-bun": "bun build.ts",
    "build-dummy": "echo build",
    "bun-build": "bun bun src/index.ts",
    "esbuild": "esbuild --bundle src/index.ts --format=esm --platform=node --outfile=dist/out.js --main-fields=main,module",
    "clean": "rm -rf node_modules",
    "run": "bun src/index.ts",
    "test": "bun test",
    "dev": "echo dev"
  },
  "devDependencies": {
    "@chialab/esbuild-plugin-meta-url": "^0.17.7",
    "@wasmin/tsconfig": "*",
    "bun-types": "^1.1.22",
    "esbuild": "^0.23.0"
  },
  "dependencies": {
    "@wasmin/bun-fs-js": "*",
    "@wasmin/fs-js": "*",
    "@wasmin/nfs-js": "*",
    "@wasmin/s3-fs-js": "*",
    "@wasmin/shell": "*",
    "@wasmin/wasi-js": "*",
    "get-random-values": "^1.2.2",
    "node-fetch": "^3.2.10",
    "web-file-polyfill": "^1.0.4"
  }
}
