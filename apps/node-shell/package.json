{
  "name": "@wasm-env/node-shell",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "build": "tsc -p .",
    "build-vite": "vite build --sourcemap && yarn tsc",
    "start": "node --enable-source-maps dist/node-shell.umd.js",
    "build-esbuild": "esbuild --bundle src/index.ts --sourcemap --platform=node --outfile=dist/index.js --main-fields=main,module",
    "start-esbuild": "node --enable-source-maps dist/index.js",
    "clean": "rm -rf dist && rm -rf node_modules",
    "run": "node dist/out.js"
  },
  "devDependencies": {
    "@types/node": "^18.14.5",
    "@wasm-env/tsconfig": "*",
    "esbuild": "^0.15.12",
    "vite": "^3.1.8"
  },
  "dependencies": {
    "@wasm-env/fs-js": "*",
    "@wasm-env/node-fs-js": "*",
    "@wasm-env/shell": "*",
    "@wasm-env/wasi-js": "*"
  }
}
