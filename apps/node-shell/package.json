{
  "name": "@wasm-env/node-shell",
  "version": "0.0.0",
  "scripts": {
    "build": "vite build && yarn tsc",
    "start": "node dist/node-shell.umd.js",
    "build-esbuild": "esbuild --bundle src/index.ts --platform=node --outfile=dist/out.js --main-fields=main,module",
    "start-esbuild": "node dist/out.js",
    "clean": "rm -rf node_modules",
    "run": "node dist/out.js"
  },
  "devDependencies": {
    "esbuild": "^0.14.39",
    "typescript": "^4.8.2",
    "vite": "^3.1.8"
  },
  "dependencies": {
    "@wasm-env/fs-js": "*",
    "@wasm-env/node-fs-js": "*",
    "@wasm-env/wasi-js": "*",
    "fetch-blob": "^3.2.0",
    "get-random-values": "^2.0.0",
    "web-file-polyfill": "^1.0.4",
    "web-streams-polyfill": "^3.2.1"
  }
}
