{
  "name": "@wasmin/node-shell",
  "version": "0.0.0",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build-tsc": "tsc -p .",
    "build-vite": "vite build --sourcemap && yarn tsc",
    "start": "node --enable-source-maps dist/node-shell.umd.js",
    "build-esbuild2": "esbuild --bundle src/index.ts --sourcemap --platform=node --outfile=dist/index.js --format=esm --main-fields=main,module",
    "build": "node esbuild.mjs",
    "clean": "rm -rf dist && rm -rf node_modules",
    "run": "node dist/out.js"
  },
  "devDependencies": {
    "@chialab/esbuild-plugin-meta-url": "^0.17.7",
    "@types/node": "^18.14.5",
    "@wasmin/tsconfig": "*",
    "esbuild": "^0.23.0",
    "vite": "^5.1.6"
  },
  "dependencies": {
    "@wasmin/fs-js": "*",
    "@wasmin/node-fs-js": "*",
    "@wasmin/shell": "*",
    "@wasmin/wasi-js": "*"
  }
}
